{
  "name": "Bit1yke",
  "description": "URL Shortening API",
  "website": "https://small-sharp-tool.com/",
  "repository": "https://github.com/Patrick-Duvall/bit1yke",
  "image": "heroku/ruby",
  "env": {
    "HEROKU_APP_NAME": {
      "required": true
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "11"
      }
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/ruby"
    }
  ]
}